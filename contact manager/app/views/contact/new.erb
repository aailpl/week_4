<div class="container">
	<div class="row">
		<div class="col-xs-12 text-center"><h1>New Event</h1></div>	
	</div>
	<div class="row">
		<div class="col-xs-3"></div>
		<div class="col-xs-6">
			<form action="/new" method="post" class="form-horizontal">
				<div class="form-group">
					<label for="name">First Name</label>
					<input  type="text" id="name" name="name" class="form-control">
				</div>
				<div class="form-group">
					  <label for="last_name">Last Name</label>
					  <input  type="text" id="last_name" name="last_name" class="form-control">
				</div>
				<div class="form-group">
					<label for="phone">Phone</label>
					<input  type="text" id="phone" name="phone" class="form-control">
				</div>
				<div class="form-group">
					  <label for="email">Email</label>
					  <input  type="text" id="email" name="email" class="form-control">
				</div>
				<div class="form-group">
					  <label for="adress">Adress</label>
					 <input  type="text" id="adress" name="adress" class="form-control">
				</div>
				<div class="text-center"><button class="btn btn-default">Add Contact</button></div>	
			</form>
		</div>
		<div class="col-xs-3"></div>
	</div>
</div>